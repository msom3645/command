  background-size: 50px 90px;\par
  background-repeat: no-repeat;\par
  border-top-left-radius: 100px;\par
  border-bottom-left-radius: 100px;\par
\}\par
.loader::after\{\par
  content:'';\par
  position: absolute;\par
  top:0;\par
  left: 50%;\par
